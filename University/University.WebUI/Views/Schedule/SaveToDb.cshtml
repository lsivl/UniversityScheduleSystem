@model University.WebUI.Models.SaveResultModel
<div id="saveResult" style="display:none">
    <p id="saveResultText" style="display:none">@Model.Message </p>

    <div id="UncorrectCells" style="display:none">

        @if ((Model.UncorrectCells != null) && (Model.UncorrectCells.Count > 0))
        {
            for (int i = 0; i < Model.UncorrectCells.Count; i++)
            {
                 <p id="errrorCell-@i" style="display:none">@Model.UncorrectCells[i].Trim()</p>
            }
        }
        
    </div>
</div>